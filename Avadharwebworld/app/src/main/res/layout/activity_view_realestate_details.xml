<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/activity_view_realestate_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    tools:context="com.avadharwebworld.avadhar.Activity.ViewRealestateDetails">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:weightSum="1">
        <com.avadharwebworld.avadhar.Support.FeedImageView
            android:layout_margin="10dp"
            android:layout_width="match_parent"
            android:visibility="visible"
            android:layout_height="140dp"
            style="@style/myImageView"
            android:layout_gravity="center"
            android:id="@+id/iv_realestate_details_feed">
        </com.avadharwebworld.avadhar.Support.FeedImageView>
        <TextView
            android:text=""
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:id="@+id/tv_realestate_details_title"
            android:textColor="@color/colorAccent"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="left"
            android:id="@+id/lv_realestate_details_name"
            android:layout_marginTop="10dp">
            <TextView
                android:text="@string/property"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_tempname"
                android:textColor="@color/colorAccent"/>
            <TextView
                android:text=""
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_marginLeft="20dp"
                android:textColor="@color/bb_darkBackgroundColor"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_property" />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="left"
            android:id="@+id/lv_realestate_details_property"
            android:layout_marginTop="10dp">
            <TextView
                android:text="@string/propertyid"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_tempcompany"
                android:textColor="@color/colorAccent"/>
            <TextView
                android:text=""
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_marginLeft="20dp"
                android:textColor="@color/bb_darkBackgroundColor"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_pid" />

            />

        </LinearLayout>
        <TextView
            android:text=""
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:id="@+id/tv_realestate_details_price"
            android:textColor="@color/colorAccent"/>
        <TextView
            android:text="@string/contactdetails"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:id="@+id/tv_realestate_details_contact"
            android:textColor="@color/bb_darkBackgroundColor"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/viewUnderline"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="left"
            android:id="@+id/lv_realestate_details_jobetype"
            android:layout_marginTop="10dp">

            <TextView
                android:text="@string/name"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_tempjobtype"
                android:textColor="@color/colorAccent"
                />

            <TextView
                android:text=""
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_marginLeft="20dp"
                android:textColor="@color/bb_darkBackgroundColor"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_name" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="left"
            android:id="@+id/lv_realestate_details_dfg"
            android:layout_marginTop="10dp">

            <TextView
                android:text="@string/contactmobile"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_relg"
                android:textColor="@color/colorAccent"
                />

            <TextView
                android:text=""
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_marginLeft="20dp"
                android:textColor="@color/bb_darkBackgroundColor"
                android:layout_height="wrap_content"
                android:id="@+id/tv_realestate_details_contactnumber" />
        </LinearLayout>
        <LinearLayout
