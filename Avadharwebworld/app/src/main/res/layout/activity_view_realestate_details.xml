                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_propertytab" />

                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:id="@+id/lv_realestate_details_tab1_"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/typeofproperty"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_tab1_tempheight"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_typeofproperty" />

                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:id="@+id/lv_realestate_details_tab1"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/price"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_tab1_tempmaritialstatus"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_pricetab" />
                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:id="@+id/lv_realestate_details_tab1_joblocation"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/typeofactivity"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_tab1_tempjoblocation"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_typeofactivity" />
                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:id="@+id/lv_realestate_details_tab1_email"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/typeofuser"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_tab1_tempemail"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_typeofuser" />

                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:id="@+id/lv_realestate_details_tab1_number"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/propertydetails"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_tab1_tempnumber"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_preperty_details" />

                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/country"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_country" />

                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:text="@string/state"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorAccent"/>
                            <TextView
                                android:text=""
                                android:layout_gravity="center_horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:textColor="@color/bb_darkBackgroundColor"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_realestate_details_state" />

                            />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="left"
                            android:layout_marginTop="10dp">
                            <TextView
