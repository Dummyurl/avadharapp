<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_jobs"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.avadharwebworld.avadhar.Activity.Jobs">
    <android.support.v7.widget.RecyclerView
        android:id="@+id/recycle_job"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical"
        android:background="@color/backgroundwhite"></android.support.v7.widget.RecyclerView>
    <TextView
        android:id="@+id/tv_job_empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:visibility="gone"
        android:textSize="20dp"
        android:textColor="@color/bb_darkBackgroundColor"
        android:text="@string/noDatafound"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/textView7"
        android:layout_below="@+id/tv_job_empty_view"
        android:layout_toRightOf="@+id/tv_job_empty_view"
        android:layout_toEndOf="@+id/tv_job_empty_view"
        android:layout_marginLeft="73dp"
        android:layout_marginStart="73dp"
        android:layout_marginTop="139dp" />
</RelativeLayout>
